<?xml version="1.0" encoding="UTF-8"?>
<xp:view
	xmlns:xp="http://www.ibm.com/xsp/core">
	<div
		class="page-header">
		<p
			class="pull-right visible-xs">
			<xp:button
				type="button"
				styleClass="btn btn-xs btn-primary"
				value="#{javascript:compositeData.buttonText}">
				<xp:this.attrs>
					<xp:attr
						name="data-toggle"
						value="offcanvas">
					</xp:attr>
				</xp:this.attrs>
			</xp:button>
		</p>

		<h3>

			<xp:text
				disableTheme="true"
				value="#{compositeData.value}" />

			&#160;
			<xp:image
				url="/champion_24x24.png"
				id="image1">
				<xp:this.rendered><![CDATA[#{javascript:try{
	compositeData.cat.equals("champions")
} catch(e){
}}]]></xp:this.rendered>
			</xp:image>

		</h3>

	</div>
</xp:view>
